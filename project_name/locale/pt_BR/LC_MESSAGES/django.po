msgid "Contact"
msgstr "Contato"

#: frontend/templates/contact/contact.html:33
msgid "Send"
msgstr "Enviar"

#: frontend/templates/mail/password-reset-subject.txt:2
#, python-format
msgid "Password reset on %(PROJECT_NAME)s"
msgstr "Redefinição de senha no %(PROJECT_NAME)s"

#: frontend/templates/mail/password-reset.html:4
msgid "Password Reset"
msgstr "Redefinir Senha"

#: frontend/templates/mail/password-reset.html:10
#, python-format
msgid ""
"\n"
"        You're receiving this email because you requested a password reset "
"for your user account\n"
"        at %(PROJECT_NAME)s\n"
"    "
msgstr ""
"\n"
"        Você está recebendo este email porque solicitou uma redefinição de "
"senha para sua conta\n"
"        no %(PROJECT_NAME)s.\n"
"    "

#: frontend/templates/mail/password-reset.html:16
msgid "To reset your password"
msgstr "Para redefinir sua senha"

#: frontend/templates/mail/password-reset.html:18
msgid "click here"
msgstr "clique aqui"

#: frontend/templates/mail/password-reset.html:22
msgid "Or you can copy & paste the following link in your browser address bar:"
msgstr ""
"Ou você pode copiar & colar o link a seguir na barra de endereço do seu "
"navegador:"

#: frontend/templates/mail/password-reset.html:28
msgid "If you did not request this change ignore this email."
msgstr "Se você não solicitou essa alteração ignore este email."

#: frontend/templates/mail/password-reset.txt:2
#, python-format
msgid ""
"\n"
"You're receiving this email because you requested a password reset\n"
"for your user account at %(PROJECT_NAME)s.\n"
msgstr ""
"\n"
"Você está recebendo este email porque solicitou uma redefinição de senha\n"
"para sua conta no %(PROJECT_NAME)s.\n"

#: frontend/templates/mail/password-reset.txt:6
msgid "Please go to the following page and choose a new password:"
msgstr "Por favor, vá para a página a seguir e escolha uma nova senha:"

#: frontend/templates/mail/password-reset.txt:10
msgid "Your username, in case you've forgotten:"
msgstr "Seu nome de usuário, caso tenha se esquecido:"

#: frontend/templates/mail/password-reset.txt:11
#, python-format
msgid ""
"\n"
"Thanks,\n"
"The %(PROJECT_NAME)s team\n"
msgstr ""
"\n"
"Obrigado,\n"
"Equipe %(PROJECT_NAME)s\n"

